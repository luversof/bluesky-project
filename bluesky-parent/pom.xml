	<artifactId>bluesky-parent</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>
	<modules>
		<module>bluesky-app</module>
		<module>bluesky-web</module>
		<module>bluesky-core</module>
		<module>bluesky-opensource</module>
		<module>bluesky-test</module>
